{
  "expo": {
    "name": "IWAProject-Front",
    "slug": "iwaproject-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hugobrun.iwaproject",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hugobrun.iwaproject",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "guardhomereactnative",
    "extra": {
      "eas": {
        "projectId": "a54aa430-6fb5-43a0-8191-5daa92d18343"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a54aa430-6fb5-43a0-8191-5daa92d18343"
    },
    "plugins": [
      "expo-font",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "L'application a besoin d'accéder à vos photos pour ajouter des images aux annonces.",
          "cameraPermission": "L'application a besoin d'accéder à votre caméra pour prendre des photos."
        }
      ]
    ]
  }
}
